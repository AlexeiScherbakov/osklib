<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>net40;net45;net47;net471;netstandard2.0;netstandard2.1;netcoreapp3.1</TargetFrameworks>
		<Description>Library for controlling Windows 10 On Screen Keyboard</Description>
		<PackageTags>osk, OnScreenKeyboard, TabTip</PackageTags>
		<Version>1.7.0-beta.1</Version>
		<LangVersion>latest</LangVersion>
		<AllowUnsafeBlocks>true</AllowUnsafeBlocks>
	</PropertyGroup>


	<ItemGroup>
		<None Include="..\key.snk" Link="key.snk" />
	</ItemGroup>

	<Import Project="$(SolutionDir)osklib.targets" />
</Project>